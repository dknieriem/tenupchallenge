{
  "name": "tenupchallenge",
  "version": "0.1.0",
  "description": "A WordPress theme for the 10Up Front-End Developer assessment",
  "main": "index.js",
  "scripts": {
    "build": "sass assets/scss/style.scss:style.css --style=expanded",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "sass assets/scss/style.scss:style.css --style=expanded --watch",
    "lint:scss": "stylelint **/*.scss",
    "lint:js": "npx eslint assets/js/*.js",
    "lint": "npm run lint:scss && npm run lint:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dknieriem/tenupchallenge.git"
  },
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "author": "Don Knieriem <donald.knieriem@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dknieriem/tenupchallenge/issues"
  },
  "homepage": "https://github.com/dknieriem/tenupchallenge#readme",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "globals": "^15.5.0",
    "sass": "^1.58.0",
    "stylelint": "^15.10.0",
    "stylelint-config-recommended-scss": "^13.1.0"
  }
}
